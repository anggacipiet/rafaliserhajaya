{
  "name": "rafaliserhajaya",
  "version": "1.0.0",
  "type": "module",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          }
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "main": "index.js",
  "scripts": {
    "start": "gulp prod:images && npm run dev",
    "dev:images": "gulp dev:images",
    "dev": "tinacms dev -c \"hugo server\"",
    "build": "gulp prod:images && tinacms build --skip-cloud-checks && hugo --gc --minify --templateMetrics --forceSyncStatic -e production",
    "optimize-images": "gulp images",
    "dev:assets": "gulp dev",
    "prod:assets": "gulp prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anggacipiet/rafaliserhajaya.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anggacipiet/rafaliserhajaya/issues"
  },
  "homepage": "https://github.com/anggacipiet/rafaliserhajaya#readme",
  "description": "",
  "dependencies": {
    "tinacms": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tinacms/cli": "^1.8.2",
    "@types/node": "^22.12.0",
    "autoprefixer": "^10.4.17",
    "core-js": "^3.35.1",
    "critical": "^7.2.1",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-fontmin": "^0.7.4",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-terser": "^2.1.0",
    "sass": "^1.84.0",
    "sharp": "^0.33.5",
    "through2": "^4.0.2"
  }
}
